{
  "buildCommand": "npm run build",
  "outputDirectory": "play.pokemonshowdown.com",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/config/(.*)",
      "dest": "/config/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/style/(.*)",
      "dest": "/style/$1"
    },
    {
      "src": "/sprites/(.*)",
      "dest": "/sprites/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
